    "better-auth": "^1.3.26",
    "better-sqlite3": "^9.6.0",
    "bullmq": "^5.4.2",
    "chokidar": "^3.6.0",
    "drizzle-kit": "^0.20.14",
    "drizzle-orm": "^0.30.8",
    "fastify": "^5.6.1",
    "ioredis": "^5.3.2",
