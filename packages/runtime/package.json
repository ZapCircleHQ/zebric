{
  "name": "@zebric/runtime",
  "version": "0.1.1",
  "description": "Zebric Runtime Engine - Core interpreter and execution engine",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "Jeff Linwood",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZapCircleHQ/zebric.git",
    "directory": "packages/runtime"
  },
  "keywords": [
    "zebric",
    "runtime",
    "engine",
    "blueprint",
    "zbl",
    "low-code",
    "workflow"
  ],
  "homepage": "https://github.com/ZapCircleHQ/zebric#readme",
  "bugs": {
    "url": "https://github.com/ZapCircleHQ/zebric/issues"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "clean": "rm -rf dist"
