{
  "name": "@zebric/runtime",
  "version": "0.1.0",
  "description": "Zebric Runtime Engine - Core interpreter and execution engine",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/csrf-protection": "^7.1.0",
    "@fastify/formbody": "^8.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^8.2.0",
    "@iarna/toml": "^2.2.5",
    "@types/ws": "^8.18.1",
    "better-auth": "^1.3.26",
    "better-sqlite3": "^9.6.0",
    "bullmq": "^5.4.2",
    "chokidar": "^3.6.0",
    "drizzle-kit": "^0.20.14",
    "drizzle-orm": "^0.30.8",
    "fastify": "^5.6.1",
    "ioredis": "^5.3.2",
    "postgres": "^3.4.4",
    "ulid": "^3.0.1",
    "ws": "^8.18.3",
    "zod": "^3.23.5"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "typescript": "^5.4.5",
    "vitest": "^1.6.1"
  }
}
