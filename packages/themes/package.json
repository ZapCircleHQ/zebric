{
  "name": "@zebric/themes",
  "version": "0.1.1",
  "description": "Zebric Themes - Built-in themes for Zebric Engine",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./default": {
      "types": "./dist/default/index.d.ts",
      "import": "./dist/default/index.js"
    },
    "./minimal": {
      "types": "./dist/minimal/index.d.ts",
      "import": "./dist/minimal/index.js"
    },
    "./dashboard": {
      "types": "./dist/dashboard/index.d.ts",
      "import": "./dist/dashboard/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "Jeff Linwood",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZapCircleHQ/zebric.git",
    "directory": "packages/themes"
  },
  "keywords": [
    "zebric",
    "themes",
    "ui",
    "styling",
    "templates",
    "zbl"
  ],
  "homepage": "https://github.com/ZapCircleHQ/zebric#readme",
  "bugs": {
    "url": "https://github.com/ZapCircleHQ/zebric/issues"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.4.5"
  }
}
